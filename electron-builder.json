{
  "productName": "catecoin-client",
  "asar": "false",
  "directories": {
    "output": "release/"
  },
  "files": [
    "**/*",
    "!**/*.ts",
    "!.github",
    "!.vscode",
    "!.code-workspace",
    "!.npmrc",
    "!*.md",
    "!package.json",
    "!package-lock.json",
    "!src/",
    "!e2e/",
    "!hooks/",
    "!_config.yml",
    "!angular.json",
    "!tsconfig*.json",
    "!tslint.json",
    "!.eslintrc.json",
    "!karma.conf.js",
    "!angular.webpack.js",
    "!*.map",
    "!catecoin_data/chain/",
    "!node_modules/ng-zorro-antd/",
    "!node_modules/date-fns/",
    "!node_modules/@angular/",
    "!node_modules/@ant-design/"
  ],
  "win": {
    "target": ["portable"]
  },
  "portable": {
    "splashImage": "dist/assets/icons/favicon.png"
  }
}
